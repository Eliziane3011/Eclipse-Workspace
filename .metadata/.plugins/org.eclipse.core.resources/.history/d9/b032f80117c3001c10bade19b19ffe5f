package com.eliziane.cursojava.aula11;

public class VariaveisPontoFlutuante {

	public static void main(String[] args) {
		
		double valorPassagem = 5.50;
		
		float valorTomate = 3.50f;
		
		System.out.println("Valor da passagem =  % .2f %n" + valorPassagem);
		
		System.out.println("Valor do tomate =  % .2f %n" + valorTomate);
		
	}

}
